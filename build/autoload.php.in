<?php
spl_autoload_register(
   function($class) {
      static $classes = null;
      if ($classes === null) {
         $classes = array(
            ___CLASSLIST___
          );
      }
      $cn = strtolower($class);
      if (isset($classes[$cn])) {
         require ___BASEDIR___$classes[$cn];
      }
   }
);

require_once 'Doctrine/Common/ClassLoader.php';

$loader = new \Doctrine\Common\ClassLoader('Doctrine');
$loader->register();
